{
  "name": "tincypics",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/s3-request-presigner": "^3.485.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.7.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "10.45.0",
    "@trpc/next": "10.45.0",
    "@trpc/react-query": "10.45.0",
    "@trpc/server": "10.45.0",
    "clsx": "^2.1.0",
    "cuid": "^3.0.0",
    "immer": "^10.0.3",
    "lucide-react": "^0.305.0",
    "next": "14.0.4",
    "next-auth": "~4.24.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-qr-code": "^2.0.12",
    "react-query": "3.39.3",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "20.10.6",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "autoprefixer": "^10.4.16",
    "eslint": "8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.33",
    "prettier": "3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "prisma": "^5.7.1",
    "tailwindcss": "^3.4.0",
    "typescript": "5.3.3"
  },
  "ct3aMetadata": {
    "initVersion": "5.15.0"
  },
  "prettier": {
    "plugins": [
      "@ianvs/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ],
    "importOrder": [
      "^(react/(.*)$)|^(react$)|^(react-native(.*)$)",
      "^(next/(.*)$)|^(next$)",
      "<THIRD_PARTY_MODULES>",
      "",
      "^~/",
      "^[../]",
      "^[./]"
    ],
    "importOrderParserPlugins": [
      "typescript",
      "jsx",
      "decorators-legacy"
    ],
    "importOrderTypeScriptVersion": "4.4.0"
  }
}
